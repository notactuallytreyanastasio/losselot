    {
      "id": 2,
      "node_type": "decision",
      "title": "Lo-fi detection approach",
      "description": "How to distinguish MP3 brick-wall cutoff from natural tape/lo-fi rolloff",
      "status": "pending",
      "created_at": "2025-12-05T17:10:59.144148-05:00",
      "updated_at": "2025-12-05T17:10:59.144148-05:00",
      "metadata_json": null
    },
    {
      "id": 3,
      "node_type": "option",
      "title": "Approach A: Temporal Cutoff Variance",
      "description": "Measure how cutoff frequency varies over time. MP3=fixed, Tape=varies. Key metric: std dev of cutoff freq across windows",
      "status": "pending",
      "created_at": "2025-12-05T17:11:00.369878-05:00",
      "updated_at": "2025-12-05T17:11:00.369878-05:00",
      "metadata_json": null
    },
    {
      "id": 4,
      "node_type": "option",
      "title": "Approach B: Cross-Frequency Coherence (CFCC)",
      "description": "Measure correlation between adjacent frequency bands. MP3=sudden decorrelation at cliff. Tape=gradual decorrelation",
      "status": "pending",
      "created_at": "2025-12-05T17:11:03.792233-05:00",
      "updated_at": "2025-12-05T17:11:03.792233-05:00",
      "metadata_json": null
    },
    {
      "id": 5,
      "node_type": "observation",
      "title": "Approach A requires per-window cutoff detection",
      "description": "Need to find -20dB point in each FFT window, track frequency, calc std dev. Adds complexity but gives strong signal for variance",
      "status": "pending",
      "created_at": "2025-12-05T17:11:17.381479-05:00",
      "updated_at": "2025-12-05T17:11:17.381479-05:00",
      "metadata_json": null
    },
    {
      "id": 6,
      "node_type": "observation",
      "title": "Approach B detects cliff via decorrelation",
      "description": "Measures correlation between adjacent bands. Cliff = sudden drop >0.25. Works with existing FFT data structure",
      "status": "pending",
      "created_at": "2025-12-05T17:11:18.722242-05:00",
      "updated_at": "2025-12-05T17:11:18.722242-05:00",
      "metadata_json": null
    },
